
Option Explicit On
Option Strict On

Public Class Maze
    Private Sub startLabel_Click(sender As Object, e As EventArgs) Handles startLabel.Click
        ' changes the cursor from
        ' default to cross

        Cursor = Cursors.Cross

    End Sub

    Public Sub New()
        ' This call is required by Windows Forms Designer.
        InitializeComponent()
        ' Add any initialization after the InitializeComponent() call.
        MoveToStart()
    End Sub

    Private Sub finishLabel_MouseEnter() Handles finishLabel.MouseEnter
        ' Show a congratulatory MessageBox, then close the form.
        MessageBox.Show("Congratulations!")
        Close()
    End Sub

    ''' <summary>
    ''' Move the mouse pointer to a point 10 pixels down and to the right
    ''' of the starting point in the upper-left corner of the maze.
    ''' </summary>
    ''' <remarks></remarks>
    Private Sub MoveToStart()
        Dim startingPoint = startLabel.Location
        startingPoint.Offset(10, 10)
        Cursor.Position = PointToScreen(startingPoint)
    End Sub


    Private Sub easterEggButton_Click(sender As Object, e As EventArgs) Handles easterEggButton.Click
        ' When the mouse pointer hits a wall or enters the panel,
        ' call the MoveToStart() method.
        MoveToStart()
        Dim startingPoint = easterEggButton.Location
        startingPoint.Offset(10, 10)
        Cursor.Position = PointToScreen(startingPoint)
        Cursor = Cursors.Cross
    End Sub


    Private Sub Label_1_MouseEnter(sender As Object, e As EventArgs) Handles Label_1.MouseEnter
        ' When the mouse pointer hits a wall or enters the panel,
        ' call the MoveToStart() method.
        MoveToStart()


    End Sub

    Private Sub Label_2_MouseEnter(sender As Object, e As EventArgs) Handles Label_2.MouseEnter
        ' When the mouse pointer hits a wall or enters the panel,
        ' call the MoveToStart() method.
        MoveToStart()
        MessageBox.Show("Uh-Oh")
        Cursor = Cursors.Default


    End Sub

    Private Sub Label_3_mouseenter(sender As Object, e As EventArgs) Handles Label_3.MouseEnter
        ' When the mouse pointer hits a wall or enters the panel,
        ' call the MoveToStart() method.
        MoveToStart()

        MessageBox.Show("Try Again")
        Cursor = Cursors.Default

    End Sub

